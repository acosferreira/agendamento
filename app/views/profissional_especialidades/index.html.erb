<h1>Listing profissional_especialidades</h1>

<table>
  <tr>
    <th>Especialidade</th>
    <th>Profissional</th>
  </tr>

<% @profissional_especialidades.each do |profissional_especialidade| %>
  <tr>
    <td><%=h profissional_especialidade.especialidade_id %></td>
    <td><%=h profissional_especialidade.profissional_id %></td>
    <td><%= link_to 'Show', profissional_especialidade %></td>
    <td><%= link_to 'Edit', edit_profissional_especialidade_path(profissional_especialidade) %></td>
    <td><%= link_to 'Destroy', profissional_especialidade, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New profissional_especialidade', new_profissional_especialidade_path %>