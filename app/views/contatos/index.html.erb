<h1>Listing contatos</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Tipo contato</th>
    <th>Paciente</th>
    <th>Estabelecimento</th>
    <th>Profissional</th>
  </tr>

<% @contatos.each do |contato| %>
  <tr>
    <td><%=h contato.name %></td>
    <td><%=h contato.tipo_contato %></td>
    <td><%=h contato.paciente_id %></td>
    <td><%=h contato.estabelecimento_id %></td>
    <td><%=h contato.profissional_id %></td>
    <td><%= link_to 'Show', contato %></td>
    <td><%= link_to 'Edit', edit_contato_path(contato) %></td>
    <td><%= link_to 'Destroy', contato, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New contato', new_contato_path %>